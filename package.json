{
  "name": "rc-css-animate",
  "author": "wsafight",
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "animate"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },

  "module": "dist/rc-css-animate.esm.js",
  "size-limit": [
    {
      "path": "dist/rc-css-animate.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rc-css-animate.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "husky": "^8.0.2",
    "dts-cli": "1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
